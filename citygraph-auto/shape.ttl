BASE <https://citygraph.co/opendata/KeyStatisticsofOilandGasActivity/>
PREFIX rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs:    <http://www.w3.org/2000/01/rdf-schema#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX schema: <http://schema.org/>
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
PREFIX cube: <https://cube.link/>
PREFIX dimension: <https://citygraph.co/opendata/KeyStatisticsofOilandGasActivity/1/dimension/>
PREFIX sh: <http://www.w3.org/ns/shacl#>
PREFIX meta: <https://cube.link/meta/>
PREFIX qudt: <http://qudt.org/schema/qudt/>
PREFIX time: <http://www.w3.org/2006/time#>
PREFIX dcat: <http://www.w3.org/ns/dcat#>
PREFIX dcterms: <http://purl.org/dc/terms/>


<1> cube:observationConstraint [
  sh:property [
    a cube:KeyDimension;
      sh:path dimension:date;
      schema:name "Date"@en;
      qudt:scaleType qudt:IntervalScale;
      sh:datatype xsd:decimal

  ],[
    a cube:KeyDimension;
      sh:path dimension:emirate;
      schema:name "Emirate"@en;
      qudt:scaleType qudt:NominalScale;
      sh:datatype xsd:string

  ],[
    a cube:KeyDimension;
      sh:path dimension:indicators;
      schema:name "Indicators"@en;
      qudt:scaleType qudt:NominalScale;
      sh:datatype xsd:string

  ],[
    a cube:MeasureDimension;
      sh:path dimension:indicators_values;
      schema:name "Indicators_values"@en;
      qudt:scaleType qudt:RatioScale;
      sh:datatype xsd:decimal

  ],[
    a cube:MeasureDimension;
      sh:path dimension:notes;
      schema:name "Notes"@en;
      qudt:scaleType qudt:NominalScale;
      sh:datatype xsd:string
  ]
] .